{
  "name": "twitf/expression-engine",
  "description": "A PHP library for parsing and executing multi-line expressions",
  "type": "library",
  "version": "1.0.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Twitf\\ExpressionEngine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "twitf"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symfony/var-dumper": "^6.4"
  }
}
